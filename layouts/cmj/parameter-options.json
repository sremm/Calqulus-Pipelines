[
	{
		"id": "base-section",
		"type": "section"
	},
	{
		"id": "top-level-section",
		"inherits": "base-section",
		"displayOptions": {
			"stacking": "vertical",
			"modeButtons": [
				"mean",
				"consistency"
			],
			"expand": "max-sidebar"
		},
		"printOptions": {
			"avoidBreaks": true,
			"avoidLeadingBreak": true
		}
	},
	{
		"id": "base-timeseries",
		"type": "timeseries",
		"displayOptions": {
			"precision": 2,
			"annotations": true,
			"modes": [
				"mean",
				"consistency"
			],
			"expand": "max-sidebar"
		},
		"chartOptions": {
			"xAxis": {
				"type": "time",
				"unit": "s"
			},
			"yAxis": {
				"title": "Angle",
				"unit": "°"
			},
			"height": "56%",
			"colorBy": "set",
			"playheads": true,
			"allowSetsToggle": true
		}
	},
	{
		"id": "timeseries-position",
		"inherits": "base-timeseries",
		"type": "timeseries",
		"chartOptions": {
			"yAxis": {
				"title": "Position",
				"unit": "mm"
			},
			"height": "56%",
			"colorBy": "set",
			"playheads": true,
			"allowSetsToggle": true
		}
	},
	{
		"id": "collapsable-tables",
