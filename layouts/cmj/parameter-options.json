	{
		"id": "base-table",
		"type": "table",
		"displayOptions": {
			"title": "Range of Motion",
			"precision": 1
		},
		"tableOptions": {
			"orientation": "column",
			"categories": {
				"enabled": false
			},
			"headers": [
				{
					"title": "",
					"span": 1
				},
				{
					"title": "Max",
					"span": 1
				},
				{
					"title": "Min",
					"span": 1
				},
				{
					"title": "Range",
					"span": 1
				}
			]
		}
	},
	{
		"id": "metrics-tables",
		"type": "table",
		"displayOptions": {
			"title": "Metrics",
			"precision": 1
		},
		"tableOptions": {
			"orientation": "column",
			"categories": {
				"enabled": false
			},
			"headers": [
				{
					"title": "",
					"span": 1
				},
				{
					"title": "Min / Max",
					"span": 1
				},
				{
					"title": "Mean +- Std",
					"span": 1
				}
			]
		},
		"dataLinks": [
			{
				"title": "",
				"values": [
					"text://Jump Height","text://Depth of Movement"
				]
			},
			{
				"title": "",
				"values": [
					"jump_height","depth_of_movement"
				],
				"aggregate": [
					"min",
					"max"
				],
				"unit": "cm"
			},
			{
				"title": "",
				"values": [
					"jump_height","depth_of_movement"
				],
				"aggregate": [
					"mean",
					"stddev"
				],
				"unit": "cm"
			}
		]
